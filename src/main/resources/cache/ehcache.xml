<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false">
	<diskStore path="java.io.tmpdir" />

	<cache name="status" timeToLiveSeconds="600" maxElementsInMemory="100"
		eternal="false" overflowToDisk="false" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="1"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="releaseVersion" timeToLiveSeconds="600"
		maxElementsInMemory="100" eternal="false" overflowToDisk="false"
		maxElementsOnDisk="10000000" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="1" memoryStoreEvictionPolicy="LRU" />

	<cache name="team" timeToLiveSeconds="600" maxElementsInMemory="100"
		eternal="false" overflowToDisk="false" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="1"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="role" timeToLiveSeconds="600" maxElementsInMemory="100"
		eternal="false" overflowToDisk="false" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="1"
		memoryStoreEvictionPolicy="LRU" />
	
	<cache name="emailconfig" timeToLiveSeconds="600" maxElementsInMemory="100"
		eternal="false" overflowToDisk="false" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="1"
		memoryStoreEvictionPolicy="LRU" />

</ehcache>
